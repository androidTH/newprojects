    <style name="TopPopAnim" parent="@android:style/Animation.Translucent">
        <item name="android:windowEnterAnimation">@anim/dd_menu_in</item>
        <item name="android:windowExitAnimation">@anim/dd_menu_out</item>
    </style>

    <style name="imageAnim" parent="@android:style/Animation.Translucent">
        <item name="android:windowEnterAnimation">@anim/img_fade_in</item>
        <item name="android:windowExitAnimation">@anim/img_fade_out</item>
    </style>

    <style name="chooseAnim" parent="Theme.AppCompat.Light.NoActionBar">
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowIsTranslucent">true</item>
        <item name="android:windowAnimationStyle">@style/TopPopAnim</item>
    </style>

    <style name="imageactivity" parent="Theme.AppCompat.Light.NoActionBar">
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowIsTranslucent">true</item>
        <item name="windowActionBar">false</item>
        <item name="android:windowBackground">@android:color/transparent</item>
        <item name="android:windowAnimationStyle">@style/imageAnim</item>
    </style>

    <style name="tv_tags_iconleft">
        <item name="android:textSize">14sp</item>
        <item name="android:textColor">@color/color_999999</item>
    </style>

    <style name="tv_tags_iconright">
        <item name="android:textSize">14sp</item>
