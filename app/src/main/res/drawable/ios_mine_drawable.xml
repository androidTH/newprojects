<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
	<item android:state_checked="true">
		<shape>
			<solid android:color="@color/color_F7AB00"/>
			<corners android:radius="99dp"/>
		</shape>
	</item>
	<item>
		<shape>
			<solid android:color="@color/color_c7c7c7"/>
			<corners android:radius="99dp"/>
			<stroke
				android:width="1dp"
				android:color="@color/color_c7c7c7"/>
		</shape>
	</item>
</selector>